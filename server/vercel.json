{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/bun"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/index.ts"
    }
  ],
  "installCommand": "bun install",
  "buildCommand": "bun run build",
  "functions": {
    "src/index.ts": {
      "runtime": "@vercel/bun@1.0.0"
    }
  }
}
